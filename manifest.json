{
  "manifest_version": 3,
  "name": "Warcraft Logs Skill Tracker",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "scripting",
	"storage"
  ],
  "host_permissions": [
    "https://www.warcraftlogs.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Skill Tracker"
  },
  "options_page": "index.html"
}